simple
  = """conl
    root = <root>
    definitions
      root
        keys
          a = test
  = """conl
    a = ; test

one of
  = """conl
    root = <root>
    definitions
      root
        keys
          a = <test>
      test
        one of
          = ca
          = cb
          = cc
  = """conl
    a = ; ca,cb,cc

one of nested
  = """conl
    root = <root>
    definitions
      root
        one of
          = <a>
          = <b>
      a
        required keys
          type = aa
      b
        required keys
          type = bb

  = """conl
    type = ; aa,bb
dot star
  = """conl
    root = <root>
    definitions
      root
        keys
          a = .*
          b = a
  = """conl
    a ;
    b ; a
ceci une pipe
  = """conl
    root = <root>
    definitions
      root
        keys
          a = one|two|three
  = """conl
    a ; one,three,two

docs
  = """conl
    root = <root>
    definitions
      root
        keys
          a = <test>

      test
        one of
          =
            matches = a
            docs = Hello!
  = """
    a = ; a "Hello!"
